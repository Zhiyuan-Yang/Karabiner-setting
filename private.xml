<?xml version="1.0"?>
<root>
  <appdef>
    <appname>SAFARI</appname>
    <equal>com.apple.Safari</equal>
  </appdef>
  <item>
    <name>Corsair K70 LUXrmedia key fix</name>
    <identifier>private.k70_media_key_fix</identifier>
    <autogen>__KeyToKey__ ConsumerKeyCode::MUSIC_PREV_18, ConsumerKeyCode::MUSIC_PREV</autogen>
    <autogen>__KeyToKey__ ConsumerKeyCode::MUSIC_NEXT_17, ConsumerKeyCode::MUSIC_NEXT</autogen>
  </item>
  <item>
    <name>NumLock as NeteaseMusic like</name>
    <identifier>private.numlock_neteasemusic_like</identifier>
    <autogen>__KeyToKey__ KeyCode::PC_KEYPAD_NUMLOCK, KeyCode::L, ModifierFlag::SHIFT_L | ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L</autogen>
  </item>
  <item>
    <name>Tenkey shortcut for Safari</name>
    <identifier>private.tenkey_safari</identifier>
    <only>SAFARI</only>
    <autogen>__KeyToKey__ KeyCode::KEYPAD_7, KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEYPAD_9, KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEYPAD_6, KeyCode::TAB, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEYPAD_4, KeyCode::TAB, ModifierFlag::CONTROL_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEYPAD_2, KeyCode::W, ModifierFlag::COMMAND_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEYPAD_8, KeyCode::T, ModifierFlag::COMMAND_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEYPAD_0, KeyCode::Z, ModifierFlag::COMMAND_L</autogen>
  </item>
</root>
